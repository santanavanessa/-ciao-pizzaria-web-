{
    "version": 2,
    "builds": [
        {
            "src": "./db/models/index.js",
            "use": "@vercel/node"
        }

    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],

    
        "rewrites":  [
          {"source": "/(.*)", "destination": "/"}
        ]
      

}